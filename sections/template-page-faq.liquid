<h1>Hello</h1>
<div class="container">
  <accordion class="accordion-flush" id="accordionFAQ">
    {% for block in section.blocks %}
      <div class="accordion-item">
        <h2 class="accordion-header" id="{{block.id}}">{{ block.settings.title }}</h2>
      </div>
    {% endfor %}
  </accordion>
</div>

{% schema %}
{
  "name": "t:sections.faq.name",
  "blocks": [
    {
      "name": "t:sections.faq.blocks.name",
      "type": "faq",
      "settings": [
        {
          "type": "text",
          "id": "title",
          "label": "t:sections.faq.blocks.question",
          "default": "FAQ Title Example"
        },
        {
          "type": "richtext",
          "id": "answer",
          "label": "t:sections.faq.blocks.answer"
        }
      ]
    }
  ]
}
{% endschema %}
